{
  "name": "mercadin",
  "version": "0.1.0",
  "homepage": "https://mercadin.herokuapp.com",
  "private": false,
  "devDependencies": {
    "enzyme": "^2.6.0",
    "gh-pages": "0.11.0",
    "react-addons-test-utils": "^15.4.0",
    "react-scripts": "0.6.1",
    "sw-precache": "^4.2.2"
  },
  "dependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && sw-precache --root='build/' --static-file-globs='build/**/!(*map*)'",
    "test": "react-scripts test --env=jsdom",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  }
